<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gcuzcano.lesson2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BActivity"
            android:label="@string/activityB"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CActivity"
            android:label="@string/activityC"
            android:parentActivityName=".BActivity" />
        <activity
            android:name=".DActivity"
            android:label="@string/activityD"
            android:parentActivityName=".BActivity" />
        <activity
            android:name=".EActivity"
            android:label="@string/activityE"
            android:parentActivityName=".BActivity"></activity>
    </application>

</manifest>